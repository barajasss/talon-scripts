app: Google Chrome
-
close tab:
    key(cmd-w)
next tab:
    key(ctrl-tab)
previous tab:
    key(ctrl-shift-tab)
first tab:
    key(cmd-1)
second tab:
    key(cmd-2)
third tab:
    key(cmd-3)
fourth tab:
    key(cmd-4)
fifth tab:
    key(cmd-5)
sixth tab:
    key(cmd-6)
seventh tab:
    key(cmd-7)
eighth tab:
    key(cmd-8)
ninth tab:
    key(cmd-9)

new tab:
    key(cmd-t)
open google:
    key(cmd-t)
    insert("google.com")
    key(enter)
open youtube:
    key(cmd-t)
    insert("youtube.com")
    key(enter)
    

search <phrase>:
    key(/)
    key(cmd-a)
    key(backspace)
    insert(phrase)
    key(enter)

open chatgpt:
    key(cmd-t)
    insert("chatgpt.com")
    key(return)
open slack:
    key(cmd-t)
    insert("https://app.slack.com/client/T02UDP8FDQD/C07PJ7A29P0")
    key(return)
open jira:
    key(cmd-t)
    insert("https://pivotech.atlassian.net/jira/software/projects/SLR/boards/4?assignee=712020%3A0f39e393-a69c-427d-a7d1-6166324b3861")
    key(return)
open uranus:
    key(cmd-t)
    insert("https://uranus-uat.stellarpos.io")
    key(return)
google <phrase>:
    key(cmd-t)
    insert("https://google.com/search?q=")
    insert(phrase)
    key(enter)
talon shortcuts:
    key(cmd-t)
    insert("https://gist.github.com/573/78048bb16de71afa586cf84a241093e4")
    key(enter)