app: code
-
escape:
    key(escape)

save changes:
    key(cmd-ctrl-s)

clear terminal:
    insert("clear")
    key(return)

load file <phrase>:
    key(cmd-p)

load project:
    key(ctrl-r)

load new instance:
    key(cmd-shift-n)

toggle terminal:
    key(cmd-j)

toggle explorer:
    key(cmd-b)

zen mode:
    key(cmd-k)
    key(z)

